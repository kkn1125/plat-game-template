    "mobx": "^6.13.5",
    "terser": "^5.37.0",
    "terser-webpack-plugin": "^5.3.11",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.6.2",
    "uuid": "^11.0.5",
